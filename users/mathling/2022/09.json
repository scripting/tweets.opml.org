                            "subs": [
                                {
                                    "text": "#GenerativeArt interlude\n\nYour friend, the polynomial spiral\n\n#XQuery #SVG <a href=\"https://t.co/tIiyx4r22x\">[link]</a>",
                                    "rawText": "#GenerativeArt interlude\n\nYour friend, the polynomial spiral\n\n#XQuery #SVG https://t.co/tIiyx4r22x",
                                    "type": "tweet",
                                    "created": "Tue, 06 Sep 2022 20:34:27 GMT",
                                    "tweetId": "1567249894334951424",
                                    "urltweet": "https://twitter.com/mathling/status/1567249894334951424",
                                    "tweetUserName": "mathling",
                                    "subs": [
                                        {
                                            "text": "Wrought + verdigris <a href=\"https://t.co/N5L8CHJaCT\">[link]</a>",
                                            "rawText": "Wrought + verdigris https://t.co/N5L8CHJaCT",
                                            "type": "tweet",
