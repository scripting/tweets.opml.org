                                            "tweetUserName": "mathling",
                                            "subs": [
                                                {
                                                    "text": "Last one: a rockier island. <a href=\"https://t.co/SrxEBnFhYW\">[link]</a>",
                                                    "rawText": "Last one: a rockier island. https://t.co/SrxEBnFhYW",
                                                    "type": "tweet",
